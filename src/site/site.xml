<project xmlns="http://maven.apache.org/DECORATION/1.6.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://maven.apache.org/DECORATION/1.6.0 http://maven.apache.org/xsd/decoration-1.6.0.xsd">
    <skin>
    	<groupId>lt.velykis.maven.skins</groupId>
    	<artifactId>reflow-maven-skin</artifactId>
    	<version>1.1.1</version>
    </skin>
	<custom>
		<reflowSkin>
			<theme>bootswatch-cosmo</theme>
			<absoluteResourceURL>http://www.deventropy.org/junit-helper/</absoluteResourceURL>
			<smoothScroll>true</smoothScroll>
			<highlightJs>true</highlightJs>
			<highlightJsTheme>github</highlightJsTheme>
			<brand>
				<!-- Brand text in top-left part of the site -->
				<name>
			          <![CDATA[
			          <span class="color-highlight">JUnit Helper</span> Utils
			          ]]>
				</name>
				<href>./</href>
			</brand>
			<slogan>Utility JUnit resource / libraries to ease testing</slogan>
			<titleTemplate>%2$s | %1$s</titleTemplate>
			<!-- Use Table of Contents at the side of the page (max 6 elements) -->
			<toc>sidebar</toc>
			<tocTopMax>6</tocTopMax>
			<!-- Include the documentation and tools in the top navigation (in addition to links) -->
			<topNav>Modules|Development</topNav>
			<!-- Split menus in the bottom navigation -->
			<bottomNav maxSpan="9">
				<column>Main|Modules</column>
				<column>Development</column>
				<column>reports|Legal</column>
			</bottomNav>
			<bottomDescription>
		        <![CDATA[
		          <span class="color-highlight">JUnit Helper</span> Utils is a collection of (mostly) External Resources
		          built for <a href="http://junit.org/" title="JUnit">JUnit</a>. They aid in developing test cases using
		          external resources, like databases, HTTP servers, etc.
		        ]]>
        	</bottomDescription>
		</reflowSkin>
	</custom>
    
	<bannerLeft>
		<name><![CDATA[<span class="color-highlight">JUnit Helper</span> Utils]]></name>
		<href>./</href>
	</bannerLeft>
	<publishDate position="bottom" format="dd MMM yyyy" />
	<version position="bottom" />
	
	<body>
		<head>
		<link rel="author" href="http://blog.bindul.name/"/>
		</head>
		
		<links>
			<item name="Development Entropy" href="../" />
			<item name="JUnit Helper @ GitHub" href="https://github.com/deventropy/junit-helper" />
		</links>
		
		<breadcrumbs>
			<item name="JUnit Helper" href="/" />
		</breadcrumbs>
		
		<menu name="Main" inherit="top">
			<item name="Home" href="./" />
			<item name="GitHub project" href="https://github.com/deventropy/junit-helper" />
		</menu>
		<menu name="Modules" inherit="top">
			<item name="JUnit Helper :: Derby" href="junit-helper-derby/index.html" />
		</menu>
		<menu name="Development" inherit="bottom">
			<item name="Development Guide" href="../development.html" />
			<item name="Issues" href="https://github.com/deventropy/junit-helper/issues" />
			<item name="Fork on GitHub" href="https://github.com/deventropy/junit-helper#fork-destination-box" />
			<item name="Continuous Integration" href="https://travis-ci.org/deventropy/junit-helper" />
		</menu>
		<menu name="Legal" inherit="bottom">
			<item name="License" href="license.html" />
			<item name="Privacy Policy" href="../privacy.html"/>
		</menu>
		<menu name="Maven documentation" ref="reports" inherit="bottom"/>
	</body>
</project>